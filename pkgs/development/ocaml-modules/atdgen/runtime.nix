{
  buildDunePackage,
  atdgen-codec-runtime,
  biniou,
  yojson,
  version ? "2.16.0",
}:
let
  atdgen-codec-runtime-override = atdgen-codec-runtime.override {
    inherit version;
  };
in
buildDunePackage {
  pname = "atdgen-runtime";
  inherit (atdgen-codec-runtime-override) version src;

  minimalOCamlVersion = "4.08";

  propagatedBuildInputs = [
    biniou
    yojson
  ];

  meta = atdgen-codec-runtime-override.meta // {
    description = "Runtime library for code generated by atdgen";
  };

}
