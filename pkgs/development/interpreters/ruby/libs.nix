        gemFlags = "--no-ri --no-rdoc";
        propagatedBuildInputs = [ libs.mime_types libs.rake ];
      };
      ncurses = { buildInputs = [ pkgs.ncurses ]; };
      ncursesw = { buildInputs = [ pkgs.ncurses ]; };
