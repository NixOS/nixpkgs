-^# Compute the installation prefix relative to this file\.\n.*?set\(_IMPORT_PREFIX ""\)\nendif\(\)
+set(_((QT_MODULE_NAME))_NIX_OUT "((NIX_OUT_PATH))")\nset(_((QT_MODULE_NAME))_NIX_DEV "((NIX_DEV_PATH))")\nset(_((QT_MODULE_NAME))_NIX_BIN "((NIX_BIN_PATH))")
/s

-\${_IMPORT_PREFIX}/(\./)?include
+\${_((QT_MODULE_NAME))_NIX_DEV}/include
/g

-\${_IMPORT_PREFIX}/(\./)?libexec
+\${_((QT_MODULE_NAME))_NIX_OUT}/libexec
/g

-\${_IMPORT_PREFIX}/(\./)?lib
+\${_((QT_MODULE_NAME))_NIX_OUT}/lib
/g

-\${_IMPORT_PREFIX}/(\./)?plugins
+\${_((QT_MODULE_NAME))_NIX_BIN}/lib/qt-((qtCompatVersion))/plugins
/g

# all qt binaries are devTools
-\${_IMPORT_PREFIX}/(\./)?bin
+\${_((QT_MODULE_NAME))_NIX_DEV}/bin
/g

-\${_IMPORT_PREFIX}/(\./)?mkspecs
+\${_((QT_MODULE_NAME))_NIX_DEV}/mkspecs
/g

-\${_IMPORT_PREFIX}/(\./)?qml
+\${_((QT_MODULE_NAME))_NIX_OUT}/lib/qt-((qtCompatVersion))/lib/qml
/g

-set\(_IMPORT_PREFIX\)
+set(_((QT_MODULE_NAME))_NIX_OUT)\nset(_((QT_MODULE_NAME))_NIX_DEV)\nset(_((QT_MODULE_NAME))_NIX_BIN)
/g

-\${QtBase_SOURCE_DIR}/libexec
+\${QtBase_BINARY_DIR}/libexec
/g

-\${QT_BUILD_INTERNALS_RELOCATABLE_INSTALL_PREFIX}/\${INSTALL_LIBEXECDIR}
+((NIX_OUT_PATH))/libexec
/g

-\${QT_BUILD_INTERNALS_RELOCATABLE_INSTALL_PREFIX}/\${INSTALL_BINDIR}
+((NIX_DEV_PATH))/bin
/g

-\${QT_BUILD_INTERNALS_RELOCATABLE_INSTALL_PREFIX}/\${INSTALL_DOCDIR}
+((NIX_OUT_PATH))/share/doc
/g

-\${QT_BUILD_INTERNALS_RELOCATABLE_INSTALL_PREFIX}/\${INSTALL_LIBDIR}
+((NIX_OUT_PATH))/lib
/g

-\${QT_BUILD_INTERNALS_RELOCATABLE_INSTALL_PREFIX}/\${INSTALL_MKSPECSDIR}
+((NIX_DEV_PATH))/mkspecs
/g

-\${CMAKE_CURRENT_LIST_DIR}/\.\./mkspecs
+((NIX_DEV_PATH))/mkspecs
/g

-\${CMAKE_CURRENT_BINARY_DIR}/mkspecs
+((NIX_DEV_PATH))/mkspecs
/g

-(  INTERFACE_INCLUDE_DIRECTORIES )
+  INTERFACE_LINK_DIRECTORIES "\${_((QT_MODULE_NAME))_NIX_OUT}/lib"\n\1
/gs

-\${CMAKE_CURRENT_LIST_DIR}/\.\./mkspecs
+((NIX_DEV_PATH))/mkspecs
/g

-\${CMAKE_CURRENT_BINARY_DIR}/mkspecs
+((NIX_DEV_PATH))/mkspecs
/g
