Function(["name","url","rev"],
  Package(
    [ ("name", Var("name"))
    , ("build", Relative("fetchsvn/fetchsvn.sh"))
    , ("url", Var("url"))
    , ("rev", Var("rev"))

    , ("svn", Call(IncludeFix("subversion/subversion.fix"), 
        [ ("localServer", True)
        , ("httpsClient", True)
        , ("httpServer", True)
        , ("pythonBindings", True)
        ]))

    ]
  )
)