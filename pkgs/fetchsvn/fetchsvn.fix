Function(["name","url","dir","rev"],
  Package(
    [ ("name", Var("name"))
    , ("build", Relative("fetchsvn/fetchsvn.sh"))
    , ("url",   Var("url"))
    , ("dir",   Var("dir"))
    , ("rev",   Var("rev"))

    , ("svn", Call(IncludeFix("subversion/subversion.fix"), 
        [ ("localServer", True)
        , ("httpsClient", True)
        , ("httpServer", True)
        , ("pythonBindings", True)
        ]))
    ]
  )
)