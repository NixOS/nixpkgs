+int
+getint(FILE *fp)
+{
+    int c, ret;
+
+    for (;;)
+    {
+	while (isspace(c = fgetc(fp)));
+	if (c == '#')
+	    while (fgetc(fp) != '\n');
+	else break;
+    }
+    if (!isdigit(c)) return -1;
+    for (ret = c-'0'; isdigit(c = fgetc(fp)); )
+	ret = ret*10 + c-'0';
+    return ret;
+}
